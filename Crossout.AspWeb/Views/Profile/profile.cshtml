@{
    Layout = "_Layout";
}

@using Crossout.AspWeb.Helper
@using Crossout.AspWeb.Models.Cod
@model UserProfile
@{
    var loc = Model.Localizations;
}

<partial name="header" model="@Model" />

<div class="half-hd-container px-3 profile">

    @if (WebSettings.Settings.EnableAds)
    {
        <partial name="leaderboardad" />
    }
    <partial name="profile_overview" model="@Model" />
    <partial name="match_history_overview" />
</div>

<script src="~/scripts/blockies.min.js?@CacheBuster.Once"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js?@CacheBuster.Once"></script>
<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css?@CacheBuster.Once" />
<script src="~/scripts/internal/profile.js?@CacheBuster.Once"></script>

<script src="~/scripts/internal/blockyplacer.js?@CacheBuster.Once"></script>
<script>
    placeBlockies();

    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })
</script>
